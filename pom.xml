        </dependency>
        <dependency>
            <artifactId>hudson-test-framework</artifactId>
            <groupId>org.eclipse.hudson</groupId>
            <type>jar</type>
            <exclusions>
                <exclusion>
                    <artifactId>hudson-maven-embedder</artifactId>
                    <groupId>org.jvnet.hudson</groupId>
                </exclusion>
            </exclusions>
        </dependency>
